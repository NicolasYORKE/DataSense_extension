{
    "name": "DataSense",
    "version": "1.0.0",
    "description": "Manage your cookies !",
    "manifest_version": 3,
    "author": "DataSense Team",
    "action":{
        "default_popup": "index.html",
        "default_title": "Manage your cookies !"
    },
    "permissions": [
        "cookies",
        "notifications",
        "webNavigation"
      ],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
    }
}